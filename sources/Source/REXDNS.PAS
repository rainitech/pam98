var
  ReportDNS: TReportDNS;

procedure RunReportDNS(const MyReportParam: TMyReportParam);

implementation

{$R *.DFM}

var X: TMyReportParam;

procedure TReportDNS.ShowReport;
begin
  if X.Obdobie1=X.Obdobie2 then
    QRLabel47.Caption := 'REKAPITULÁCIA MIEZD ZA ' +
       Mesiac(X.Obdobie1 mod 100,true)+' '+IntToStr(X.Obdobie1 div 100)
  else
    QRLabel47.Caption := 'REKAPITULÁCIA MIEZD ZA ' +
       Mesiac(X.Obdobie1 mod 100,true)+' '+IntToStr(X.Obdobie1 div 100) +
       ' - '+
       Mesiac(X.Obdobie2 mod 100,true)+' '+IntToStr(X.Obdobie2 div 100);

  Q0_.Close;
  Q0_.ParamByName('P1').AsInteger := X.Obdobie1;
  Q0_.ParamByName('P1X').AsInteger := X.Obdobie2;
  Q0_.ParamByName('P2').AsInteger := X.AktMinSid;
  Q0_.ParamByName('P3').AsInteger := X.AktMaxSid;
  Q0_.Open;
  Preview;
  Q0_.Close;
end;


procedure TReportDNS.Q0_CalcFields(DataSet: TDataSet);
begin
  Q0_CALC_FIRMA.Value := X.AktFirmaStr;

  Q0_CALC_MPT.Value:=COPY(
    Trim(Q0_Name.Value)+' '+
    Trim(Q0_Surname.Value)+' '+
    Trim(Q0_Title.Value),
    1,100);

  Q0_CALC_ZRAZKY.Value :=
    Q0_Sporenie.Value +
    Q0_Ne_stravne.Value +
    Q0_Ne_poistovna.Value +
    Q0_Ne_pozicky.Value +
    Q0_Ne_odborar.Value +
    Q0_Ne_exekucia.Value +
    Q0_Ne_zaloha.Value;

  if Q0_Dan_vyrovnana.Value=1
  then Q0_CALC_brutto_dohody.Value:=Q0_N_brutto.Value
  else Q0_CALC_brutto_dohody.Value:=0;

end;

procedure RunReportDNS(const MyReportParam: TMyReportParam);
begin
  X:=MyReportParam;
  Screen.Cursor:=crHourGlass;
  try
  try
    ReportDNS:=TReportDNS.Create(Application);
    if X.DisableShape then QRDisableShape(ReportDNS);
    if X.DisableLabel then QRDisableLabel(ReportDNS);
    Screen.Cursor:=crDefault;
    ReportDNS.ShowReport;
  except on E:Exception do begin MyError(SMyAppReportShowFailed+E.Message); end;
  end;
  finally
    Screen.Cursor:=crDefault;
    ReportDNS.Free;
  end;
end;

end.
